{
	"name": "zyrakia.github.io",
	"private": "true",
	"scripts": {
		"start": "yarn clean && yarn parcel-dev",
		"build": "yarn clean ** yarn clean-build && parcel-build",
		"parcel-build": "parcel build ./index.html --dist-dir docs",
		"parcel-dev": "parcel ./index.html --open",
		"clean": "yarn clean-build-cache && yarn clean-dev-build",
		"clean-build-cache": "rimraf .parcel-cache",
		"clean-build": "rimraf docs",
		"clean-dev-build": "rimraf dist"
	},
	"dependencies": {},
	"devDependencies": {
		"parcel": "^2.0.0-beta.2",
		"rimraf": "^3.0.2"
	},
	"license": "MIT"
}
